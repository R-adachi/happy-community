<p id="notice"><%= notice %></p>

<h1>コミュニティタグ</h1>

<table>
  <thead>
    <tr>
      <th>タグID</th>
      <th>コミュニティID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @community_tags.each do |community_tag| %>
      <tr>
        <td><%= community_tag.tag_id %></td>
        <td><%= community_tag.community_id %></td>
        <td><%= link_to 'Show', community_tag %></td>
        <td><%= link_to 'Edit', edit_community_tag_path(community_tag) %></td>
        <td><%= link_to 'Destroy', community_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

